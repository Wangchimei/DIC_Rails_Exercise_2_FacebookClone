<%= form_with(model: feed, local: :ture, url:new_or_edit) do |f|%>

<% if @feed.errors.any? %>
  <div class="">
    <% @feed.errors.full_messages.each do |message| %>
      <p><%=message%></p>
    <%end%>
  </div>
<%end%>

<div class="">
  <%=f.label :title%>
  <%=f.text_field :title%>
</div>

<div class="">
  <%=f.label :content%>
  <%=f.text_area :content, size: '50x10'%>
</div>

<div class="">
  <%=f.label :image%>
  <%=f.text_field :image%>
</div>

<%# TAKE OUT LATER %>
<div class="">
  <%=f.text_field :user_id, value:"1"%>
</div>

<div class="">
  <%= f.submit button_text%>
</div>

<%end%>

<%= link_to "戻る", feeds_path%>
