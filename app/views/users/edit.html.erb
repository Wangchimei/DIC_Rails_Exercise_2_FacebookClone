<h1>アカウント更新</h1>

<%= form_with(model:@user, local: true) do |f| %>
<div>
  <%= f.label :name, "名前"%>
  <%= f.text_field :name%>
</div>
<div>
  <%= f.label :email, "メールアドレス"%>
  <%= f.text_field :email%>
</div>
<div>
  <%= f.hidden_field :password, value: "#{@user.password_digest}"%>
</div>
<%= f.submit%>
<%end%>
